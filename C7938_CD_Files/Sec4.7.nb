       RowBox[{"-", ".01"}], ",", ".08"}], "}"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"1", "/", "2"}], ",", 
      RowBox[{"1", "/", "4"}], ",", 
      RowBox[{"1", "/", "4"}]}], "}"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"A", ".", "sigma", ".", 
