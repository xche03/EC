      RowBox[{"2", "\[Times]", 
       SuperscriptBox["10", 
        RowBox[{"-", "6"}]]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"2", "\[Times]", 
