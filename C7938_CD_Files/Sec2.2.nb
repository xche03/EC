Cell[TextData[{
 "The binomial mean is of course ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[CenterDot]", "p"}], TraditionalForm]]],
 ", which we prove in the next theorem.  We also include the formula for the \
variance of the binomial distribution. "
}], "Text",
 CellChangeTimes->{3.424494051859375*^9}],

Cell[TextData[{
 StyleBox["Theorem 1",
  FontWeight->"Bold"],
 ".  If ",
 StyleBox["X",
  FontSlant->"Italic"],
 " \[Tilde] b(",
 StyleBox["n",
  FontSlant->"Italic"],
 ", ",
 StyleBox["p",
  FontSlant->"Italic"],
 "), then (a) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["E",
      FontSlant->"Plain"], "[", "X", "]"}], " ", "=", " ", 
    RowBox[{"n", "\[CenterDot]", "p"}]}], TraditionalForm]]],
 " ; (b) Var(",
 StyleBox["X",
  FontSlant->"Italic"],
 ") = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[CenterDot]", "p", "\[CenterDot]", 
    RowBox[{"(", 
     RowBox[{"1", "-", "p"}], ")"}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{3.424494058953125*^9}],

Cell[TextData[{
 StyleBox["Proof",
  FontWeight->"Bold"],
 ".  (a) We may factor ",
 StyleBox["n",
  FontSlant->"Italic"],
 " and ",
 StyleBox["p",
  FontSlant->"Italic"],
 " out of the sum ",
 StyleBox["that defines ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["E",
     FontSlant->"Plain"], "[", "X", "]"}], TraditionalForm]],
  FontColor->GrayLevel[0]],
 ", delete the ",
 StyleBox["k",
  FontSlant->"Italic"],
 " = 0 term (which equals 0), and cancel the common factors of ",
 StyleBox["k",
  FontSlant->"Italic"],
 " to get"
}], "Text",
 CellChangeTimes->{{3.4244941024375*^9, 3.42449411359375*^9}, {
  3.436135600347519*^9, 3.4361356003555193`*^9}}],

Cell[BoxData[
 FormBox[Cell[TextData[Cell[BoxData[
   FormBox[GridBox[{
      {
       RowBox[{
        StyleBox["E",
         FontSlant->"Plain"], "[", "X", "]"}], "=", 
       RowBox[{
        UnderoverscriptBox["\[CapitalSigma]", 
         RowBox[{"k", "=", "0"}], "n"], " ", 
        RowBox[{"k", "\[CenterDot]", 
         RowBox[{"(", "\[NegativeThinSpace]", GridBox[{
            {
             RowBox[{" ", "n", " "}]},
            {"k"}
           }], "\[NegativeThinSpace]", ")"}]}], 
        FormBox[
         SuperscriptBox["p", "k"],
         TraditionalForm], 
        FormBox[
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", " ", "-", " ", "p"}], ")"}], 
          RowBox[{"n", "-", "k"}]],
         TraditionalForm]}]},
      {" ", "=", 
       RowBox[{
        RowBox[{"n", "\[CenterDot]", "p", "\[CenterDot]", 
         UnderoverscriptBox["\[CapitalSigma]", 
          RowBox[{"k", "=", "1"}], "n"]}], 
        FormBox[
         RowBox[{
          FractionBox[
           RowBox[{
            RowBox[{"(", 
             RowBox[{"n", "-", "1"}], ")"}], "!"}], 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"k", "-", "1"}], ")"}], "!"}], 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"n", "-", "k"}], ")"}], "!"}]}]], 
          SuperscriptBox["p", 
           RowBox[{"k", "-", "1"}]]}],
         TraditionalForm], 
        FormBox[
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", " ", "-", " ", "p"}], ")"}], 
          RowBox[{"n", "-", "k"}]],
         TraditionalForm]}]}
     },
     GridBoxAlignment->{
      "Columns" -> {{Left}}, "ColumnsIndexed" -> {}, "Rows" -> {{Baseline}}, 
       "RowsIndexed" -> {}}], TraditionalForm]]]]], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.436135610889519*^9, 3.4361356112945194`*^9}}],

Cell[TextData[{
 "Then we can change variables in the sum to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"l", "=", 
    RowBox[{"k", "-", "1"}]}], TraditionalForm]]],
 ", noting that in the exponent of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "-", "p"}], TraditionalForm]]],
 " we can rewrite ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "k"}], TraditionalForm]]],
 " as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "1", "-", 
    RowBox[{"(", 
     RowBox[{"k", "-", "1"}], ")"}]}], TraditionalForm]]],
 ", which reduces the sum to that of all terms of the b",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"n", "-", "1"}], ",", " ", "p"}], ")"}], TraditionalForm]]],
 " probability mass function."
}], "Text",
