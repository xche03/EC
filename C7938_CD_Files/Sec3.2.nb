      SuperscriptBox["e", 
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"x", "/", "\[Beta]"}], ")"}], "\[Alpha]"]}]]}]}]}], ",", 
