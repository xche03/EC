 RowBox[{
  UnderoverscriptBox["\[Integral]", "0", ".5"], 
  RowBox[{
   UnderoverscriptBox["\[Integral]", 
    RowBox[{"2", "u"}], "1"], " ", 
   RowBox[{"2", " ", 
    RowBox[{"\[DifferentialD]", "v"}], " ", 
    RowBox[{"\[DifferentialD]", "u"}]}]}]}]], "Input",
