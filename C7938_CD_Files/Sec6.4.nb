Cell[TextData[{
 StyleBox["Example 4",
  FontWeight->"Bold"],
 "  Assume that the logged GE prices as in Figure 12 do form a Brownian \
motion.  Estimate the parameters \[Mu] and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Sigma]", "2"], TraditionalForm]]],
 ", and use the estimates to compute the probability that the price after 100 \
weeks is at least 35, given that the initial price is 32. "
}], "Text",
 CellChangeTimes->{{3.4337740523570004`*^9, 3.43377406413*^9}, {
  3.433774111278*^9, 3.433774176792*^9}, {3.433774217763*^9, 
  3.433774301506*^9}}],

Cell[TextData[{
 "\tLet ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"(", 
     SubscriptBox["P", "t"], ")"}], 
    RowBox[{"t", "\[GreaterEqual]", "0"}]], TraditionalForm]]],
 " denote the process of prices, and let ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"(", 
     SubscriptBox["X", "t"], ")"}], 
    RowBox[{"t", "\[GreaterEqual]", "0"}]], TraditionalForm]]],
 " denote the process of logged prices, i.e., for each ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "\[GreaterEqual]", "0"}], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["X", "t"], "=", 
    RowBox[{"log", "(", 
     SubscriptBox["P", "t"], ")"}]}], TraditionalForm]]],
 ".  We are assuming that ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"(", 
     SubscriptBox["X", "t"], ")"}], 
    RowBox[{"t", "\[GreaterEqual]", "0"}]], TraditionalForm]]],
 " forms a Brownian motion with parameters \[Mu] and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Sigma]", "2"], TraditionalForm]]],
 ", hence the increments ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["X", "2"], "-", 
     SubscriptBox["X", "1"]}], ",", 
    RowBox[{
     SubscriptBox["X", "3"], "-", 
     SubscriptBox["X", "2"]}], ",", 
    RowBox[{
     SubscriptBox["X", "4"], "-", 
     SubscriptBox["X", "3"]}], ",", "..."}], TraditionalForm]]],
 ", for which the length of the time interval is exactly 1 week, must have \
the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "(", 
    RowBox[{
     RowBox[{"\[Mu]", "\[CenterDot]", "1"}], ",", 
     RowBox[{
      SuperscriptBox["\[Sigma]", "2"], "\[CenterDot]", "1"}]}], ")"}], 
   TraditionalForm]]],
 " distribution, and they are independent of each other.  We can therefore \
estimate the parameters by the sample mean and sample variance of these \
increments."
}], "Text",
 CellChangeTimes->{{3.433867215894*^9, 3.433867299498*^9}, {
   3.4338673421400003`*^9, 3.433867527102*^9}, {3.433867712454*^9, 
   3.43386773684*^9}, 3.433867957241*^9, {3.4343808056102*^9, 
   3.4343808066952*^9}, {3.4343808434632*^9, 3.4343808442441998`*^9}, 
   3.4571018408570004`*^9}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Xtprocess", "=", 
   RowBox[{"Log", "[", "GEprices", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Increments", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Xtprocess", "[", 
       RowBox[{"[", "i", "]"}], "]"}], "-", 
      RowBox[{"Xtprocess", "[", 
       RowBox[{"[", 
        RowBox[{"i", "-", "1"}], "]"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "2", ",", 
       RowBox[{"Length", "[", "Xtprocess", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"xbar", "=", 
  RowBox[{"Mean", "[", "Increments", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"var", "=", 
  RowBox[{"Variance", "[", "Increments", "]"}]}]}], "Input",
 CellFrame->True,
 InitializationCell->True,
 CellChangeTimes->{{3.4335465785928*^9, 3.4335465827128*^9}, {
  3.433867987266*^9, 3.433868046815*^9}, {3.433868081927*^9, 
  3.433868106597*^9}},
 AutoStyleOptions->{"FunctionLocalVariableStyle"->None,
 "FunctionNotebookStyle"->None,
