Cell[BoxData[
 RowBox[{"1", "-", 
  RowBox[{"CDF", "[", 
   RowBox[{
    RowBox[{"NormalDistribution", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Log", "[", "32", "]"}], "+", 
       RowBox[{"xbar", "*", "100"}]}], ",", 
      SqrtBox[
       RowBox[{"var", "*", "100"}]]}], "]"}], ",", 
