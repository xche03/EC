Cell[TextData[{
 StyleBox["Example 2",
  FontWeight->"Bold"],
 "  Let ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"(", 
     SubscriptBox["X", "t"], ")"}], 
    RowBox[{"t", "\[GreaterEqual]", "0"}]], TraditionalForm]]],
 " be a standard Brownian motion starting at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "0"}], TraditionalForm]]],
 ". Find (a) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["P",
     FontSlant->"Plain"], "[", 
    RowBox[{
     SubscriptBox["X", "1.2"], ">", ".5"}], "]"}], TraditionalForm]]],
 " ; (b) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["P",
     FontSlant->"Plain"], "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["X", "5.4"], "-", 
       SubscriptBox["X", "2.3"]}], "\[LessEqual]", 
      RowBox[{"-", "1.6"}]}], ",", 
     RowBox[{
      SubscriptBox["X", "1"], ">", "0"}]}], "]"}], TraditionalForm]]],
 "; (c) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["P",
     FontSlant->"Plain"], "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["X", "2.6"], ">", "2"}], "|", 
      SubscriptBox["X", "1.1"]}], "=", "1"}], "]"}], TraditionalForm]]],
 "."
