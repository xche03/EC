Cell[TextData[{
 StyleBox["Figure 4.31",
  FontWeight->"Bold"],
 " -",
 StyleBox[" 90% of the area under the ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Chi]", "2"], "(", "28", ")"}], TraditionalForm]]],
 " density",
 " "
}], "Text",
 CellChangeTimes->{{3.4376774451199226`*^9, 3.437677451233923*^9}},
