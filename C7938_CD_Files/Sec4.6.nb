Cell[TextData[{
 "4.6 Chi-Square, Student's ",
 StyleBox["t",
  FontSlant->"Italic"],
 ", and ",
 StyleBox["F",
  FontSlant->"Italic"],
 "-Distributions"
}], "Section",
 CellFrameColor->GrayLevel[0],
 CellChangeTimes->{{3.4293035060187163`*^9, 3.4293035082207165`*^9}, {
   3.4318969853411417`*^9, 3.4318969899021416`*^9}, 3.4567789640201263`*^9},
 TextAlignment->Center,
 FontSize->14,
