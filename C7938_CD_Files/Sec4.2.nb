Cell["\<\
\tNow let us estimate the bivariate normal parameters.  We use the Mean, \
StandardDeviation, and Correlation commands as before.\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"{", 
   RowBox[{
    SubscriptBox["\[Mu]", "1"], ",", 
    SubscriptBox["\[Mu]", "2"], ",", 
    SubscriptBox["\[Sigma]", "1"], ",", 
    SubscriptBox["\[Sigma]", "2"], ",", "\[Rho]"}], "}"}], "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"Mean", "[", "lognewmex", "]"}], ",", 
    RowBox[{"Mean", "[", "logus", "]"}], ",", 
    RowBox[{"StandardDeviation", "[", "lognewmex", "]"}], ",", 
    RowBox[{"StandardDeviation", "[", "logus", "]"}], ",", 
    RowBox[{"Correlation", "[", 
     RowBox[{"lognewmex", ",", "logus"}], "]"}]}], "}"}]}]], "Input",
 CellFrame->True,
 InitializationCell->True,
 AutoStyleOptions->{"FunctionLocalVariableStyle"->None,
 "FunctionNotebookStyle"->None,
