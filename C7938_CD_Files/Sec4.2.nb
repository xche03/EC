   3.456670564792*^9}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"q", "(", 
    RowBox[{"y", "|", "x"}], ")"}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"f", "(", 
      RowBox[{"x", ",", "y"}], ")"}], 
     RowBox[{"p", "(", "x", ")"}]], "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{
       SqrtBox[
        RowBox[{"2", "\[Pi]"}]], 
