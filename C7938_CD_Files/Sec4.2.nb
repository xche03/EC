Cell[TextData[{
 "The probability mass is concentrated most densely in elliptical regions \
around the point (0, 2), which is the mean. The major and minor axes are \
tilted, and there is more spread in the ",
 StyleBox["y",
  FontSlant->"Italic"],
 "-direction than in the ",
 StyleBox["x",
  FontSlant->"Italic"],
 "-direction.  (Look at the tick marks carefully.)  Simulated data from this \
distribution show the same tendency.  Here are 200 points randomly sampled \
from the same distribution and plotted."
}], "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"simlist", "=", 
   RowBox[{"RandomReal", "[", 
    RowBox[{
     RowBox[{"MultinormalDistribution", "[", 
      RowBox[{"meanvector", ",", "covariancematrix"}], "]"}], ",", 
     RowBox[{"{", "200", "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{"ListPlot", "[", 
  RowBox[{"simlist", ",", 
   RowBox[{"PlotStyle", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"PointSize", "[", "0.015`", "]"}], "}"}]}]}], "]"}]}], "Input",
