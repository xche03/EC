Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    StyleBox["E",
     FontSlant->"Plain"], "[", 
    RowBox[{"V", "-", "U"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Integral]", "0", "1"], 
