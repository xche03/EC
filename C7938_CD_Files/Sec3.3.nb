           RowBox[{"x", ",", "y"}], ")"}], "d", " ", "y", " ", "d", " ", 
          "x"}]}]}]},
      {" ", "=", 
       RowBox[{
        SubscriptBox["\[Integral]", 
         SubscriptBox["E", "x"]], 
        RowBox[{
         SubscriptBox["\[Integral]", 
          SubscriptBox["E", "y"]], 
         RowBox[{
          RowBox[{"g", "(", "x", ")"}], 
          RowBox[{"h", "(", "y", ")"}], 
          RowBox[{"p", "(", "x", ")"}], 
          RowBox[{"q", "(", "y", ")"}], "d", " ", "y", " ", "d", " ", 
          "x"}]}]}]},
      {" ", "=", 
       RowBox[{
        SubscriptBox["\[Integral]", 
         SubscriptBox["E", "x"]], 
        RowBox[{
         RowBox[{"g", "(", "x", ")"}], 
         RowBox[{"p", "(", "x", ")"}], "d", " ", "x", 
         RowBox[{
          SubscriptBox["\[Integral]", 
           SubscriptBox["E", "y"]], 
          RowBox[{
           RowBox[{"h", "(", "y", ")"}], 
           RowBox[{"q", "(", "y", ")"}], "d", " ", "y"}]}]}]}]},
      {" ", "=", 
       RowBox[{
