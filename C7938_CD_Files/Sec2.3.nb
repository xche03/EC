Cell[TextData[{
 "Write down and simplify an expression for Var(",
 StyleBox["X",
  FontSlant->"Italic"],
 " + ",
 StyleBox["Y",
  FontSlant->"Italic"],
 ") = E[",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"{", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"X", "+", "Y"}], ")"}], "-", 
