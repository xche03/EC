Cell[TextData[{
 StyleBox["Example 4",
  FontWeight->"Bold"],
 "  If (",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", "t"], TraditionalForm]]],
 ") is a Poisson process with rate \[Lambda], find Cov(",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", "t"], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", 
    RowBox[{"t", "+", "s"}]], TraditionalForm]]],
 "). "
}], "Text",
 CellChangeTimes->{{3.4258346437469997`*^9, 3.425834655606*^9}, {
   3.4324763096152*^9, 3.4324763451432*^9}, 3.4324824101252003`*^9, {
   3.432830417234914*^9, 3.4328304230119133`*^9}}],

Cell[TextData[{
 "\tWe know that since the value of the process at time ",
 StyleBox["t",
  FontSlant->"Italic"],
 " has the Poisson distribution with parameter \[Lambda]",
 StyleBox["t",
  FontSlant->"Italic"],
 ",  E[",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", "t"], TraditionalForm]]],
 "] = \[Lambda]",
 StyleBox["t ",
  FontSlant->"Italic"],
 "and E[",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", 
    RowBox[{"t", "+", "s"}]], TraditionalForm]]],
 "] = \[Lambda](",
 StyleBox["t",
  FontSlant->"Italic"],
 "+",
 StyleBox["s",
  FontSlant->"Italic"],
 ").  It remains to compute E[",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", "t"], TraditionalForm]]],
 " \[CenterDot] ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", 
    RowBox[{"t", "+", "s"}]], TraditionalForm]]],
 "]. But also we note that ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", 
    RowBox[{"t", "+", "s"}]], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox["-", TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", "t"], TraditionalForm]]],
 " is independent of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", "t"], TraditionalForm]]],
 " and has the Poisson(\[Lambda]",
 StyleBox["s",
  FontSlant->"Italic"],
 ") distribution; hence, "
}], "Text",
