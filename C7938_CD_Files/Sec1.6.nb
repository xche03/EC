Cell[TextData[{
 "\tThe next ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " function simulates a sample of a desired size (such as 80), in sequence \
and with replacement, from the population categorized as in the universe \
table, tallies up the frequencies in each category for the sample, and \
presents the results in tabular format.  After initializing the counter \
variables to zero, it selects a random number between 0 and 1, and based on \
its value, increments one of the counters. The cutoffs used in the Which \
function are chosen so that the category probabilities of .3, .3, .2, and .2 \
are modeled. The marginal totals are then computed for the tabular display, \
and the output is done. "
}], "Text",
 CellChangeTimes->{{3.424120464203125*^9, 3.424120474359375*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"SimContingencyTable", "[", "sampsize_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "A1", ",", "A2", ",", "B1", ",", "B2", ",", "A1B1", ",", "A1B2", ",", 
      "A2B1", ",", "A2B2", ",", "nextsample", ",", "i", ",", "outtable"}], 
     "}"}], ",", 
    RowBox[{
     RowBox[{"A1B1", "=", "0"}], ";", 
     RowBox[{"A1B2", "=", "0"}], ";", 
     RowBox[{"A2B1", "=", "0"}], ";", 
     RowBox[{"A2B2", "=", "0"}], ";", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"nextsample", "=", 
         RowBox[{"RandomReal", "[", "]"}]}], ";", 
        RowBox[{"Which", "[", 
         RowBox[{
          RowBox[{"nextsample", "<", "0.3`"}], ",", 
          RowBox[{"A1B1", "=", 
           RowBox[{"A1B1", "+", "1"}]}], ",", 
          RowBox[{"0.3`", "\[LessEqual]", "nextsample", "<", "0.6`"}], ",", 
          RowBox[{"A1B2", "=", 
           RowBox[{"A1B2", "+", "1"}]}], ",", 
          RowBox[{"0.6`", "\[LessEqual]", "nextsample", "<", "0.8`"}], ",", 
          RowBox[{"A2B1", "=", 
           RowBox[{"A2B1", "+", "1"}]}], ",", 
          RowBox[{"0.8`", "\[LessEqual]", "nextsample", "\[LessEqual]", "1"}],
           ",", 
          RowBox[{"A2B2", "=", 
           RowBox[{"A2B2", "+", "1"}]}]}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "sampsize"}], "}"}]}], "]"}], ";", 
     RowBox[{"A1", "=", 
      RowBox[{"A1B1", "+", "A1B2"}]}], ";", 
     RowBox[{"A2", "=", 
      RowBox[{"A2B1", "+", "A2B2"}]}], ";", 
     RowBox[{"B1", "=", 
      RowBox[{"A1B1", "+", "A2B1"}]}], ";", 
     RowBox[{"B2", "=", 
      RowBox[{"A1B2", "+", "A2B2"}]}], ";", 
     RowBox[{"outtable", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\"\<A/B\>\"", ",", "1", ",", "2", ",", "\"\<Total\>\""}], 
         "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", "A1B1", ",", "A1B2", ",", "A1"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", "A2B1", ",", "A2B2", ",", "A2"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\"\<Total\>\"", ",", "B1", ",", "B2", ",", "sampsize"}], 
         "}"}]}], "}"}]}], ";", 
     RowBox[{"TableForm", "[", 
      TagBox["outtable",
       Function[BoxForm`e$, 
        TableForm[BoxForm`e$]]], "]"}]}]}], "]"}]}]], "Input",
 CellFrame->True,
 InitializationCell->True,
 CellChangeTimes->{{3.3986158681044674`*^9, 3.3986158754997206`*^9}},
