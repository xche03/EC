     FractionBox["1", 
      RowBox[{"2", " "}]]}],
    TraditionalForm], "+", " ", 
   FormBox[
    RowBox[{
     FractionBox["1", "4"], "\[CenterDot]", 
     FractionBox["1", "3"], "\[CenterDot]", 
     FractionBox["1", "2"], "\[CenterDot]", 
     FractionBox["1", "2"]}],
    TraditionalForm], " ", "+", 
   FormBox[
    RowBox[{" ", 
