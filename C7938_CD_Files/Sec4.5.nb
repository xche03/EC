  FormBox[
   RowBox[{
    StyleBox["P",
     FontSlant->"Plain"], "[", 
    RowBox[{"X", ">", "5"}], "]"}], TraditionalForm]]],
 ";  (c) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["P",
     FontSlant->"Plain"], "[", 
    RowBox[{"3", "\[LessEqual]", "X", "\[LessEqual]", "4"}], "]"}], 
   TraditionalForm]]],
 "."
}], "Text",
