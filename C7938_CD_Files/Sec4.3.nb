 Cell[BoxData[
  FormBox[
   RowBox[{"F", "(", "v", ")"}], TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[" of ",
  FontColor->GrayLevel[0]],
 StyleBox["V ",
