 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["P",
     FontSlant->"Plain"], "A"], TraditionalForm]]],
 " gives no probability weight to outcomes \[Omega] that are not in ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]]],
 ", hence ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"Y", "(", "\[Omega]", ")"}], 
     RowBox[{
      SubscriptBox[
       StyleBox["P",
        FontSlant->"Plain"], "A"], "[", "\[Omega]", "]"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"Y", "(", "\[Omega]", ")"}], "\[CenterDot]", 
      RowBox[{
       SubscriptBox["I", "A"], "(", "\[Omega]", ")"}], "\[CenterDot]", 
      RowBox[{
       SubscriptBox[
        StyleBox["P",
         FontSlant->"Plain"], "A"], "[", "\[Omega]", "]"}]}], "=", 
     FractionBox[
      RowBox[{
       RowBox[{"Y", "(", "\[Omega]", ")"}], "\[CenterDot]", 
       RowBox[{
        SubscriptBox["I", "A"], "(", "\[Omega]", ")"}], "\[CenterDot]", 
       RowBox[{
        StyleBox["P",
         FontSlant->"Plain"], "[", 
        RowBox[{"\[Omega]", "\[Intersection]", "A"}], "]"}]}], 
      RowBox[{
       StyleBox["P",
        FontSlant->"Plain"], "[", "A", "]"}]]}]}], TraditionalForm]]],
 ".  For every outcome \[Omega], ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["I", "A"], "(", "\[Omega]", ")"}], "\[CenterDot]", 
     RowBox[{
      StyleBox["P",
       FontSlant->"Plain"], "[", 
      RowBox[{"\[Omega]", "\[Intersection]", "A"}], "]"}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["I", "A"], "(", "\[Omega]", ")"}], "\[CenterDot]", 
     RowBox[{
      StyleBox["P",
       FontSlant->"Plain"], "[", "\[Omega]", "]"}]}]}], TraditionalForm]]],
 ", since both sides are ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["P",
     FontSlant->"Plain"], "[", "\[Omega]", "]"}], TraditionalForm]]],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Omega]", "\[Element]", "A"}], TraditionalForm]]],
 " and zero if not. Thus,"
}], "Text",
