         TraditionalForm]}], ")"}],
      TraditionalForm], 
     FormBox[
      RowBox[{"(", 
       RowBox[{"Y", "-", 
        FormBox[
         FormBox[
          SubscriptBox["\[Mu]", "y"],
          TraditionalForm],
         TraditionalForm]}], ")"}],
      TraditionalForm]}], "]"}], TraditionalForm]]],
 " = 0.  Generalize: show that if ",
 StyleBox["X",
  FontSlant->"Italic"],
 " and ",
 StyleBox["Y",
  FontSlant->"Italic"],
 " are independent, then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["E",
      FontSlant->"Plain"], "[", 
     RowBox[{
      RowBox[{"g", "(", "X", ")"}], 
      RowBox[{"h", "(", "Y", ")"}]}], "]"}], "=", 
    RowBox[{
     RowBox[{
      StyleBox["E",
       FontSlant->"Plain"], "[", 
      RowBox[{"g", "(", "X", ")"}], "]"}], 
     RowBox[{
      StyleBox["E",
       FontSlant->"Plain"], "[", 
      RowBox[{"h", "(", "Y", ")"}], "]"}]}]}], TraditionalForm]]],
 "."
}], "Text",
 CellFrame->True],

Cell["\<\
\tTheorem 2 also extends easily to the case of linear combinations of many \
random variables.\
