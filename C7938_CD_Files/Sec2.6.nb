           RowBox[{
            RowBox[{"(", 
             RowBox[{"X", "-", 
              FormBox[
               SubscriptBox["\[Mu]", "x"],
               TraditionalForm]}], ")"}], 
            RowBox[{"(", 
             RowBox[{"Y", "-", 
              FormBox[
               SubscriptBox["\[Mu]", "y"],
               TraditionalForm]}], ")"}]}], "]"}]}], 
         RowBox[{
          SubscriptBox["\[Sigma]", 
           StyleBox["x",
            FontSlant->"Italic"]], 
          SubscriptBox["\[Sigma]", 
