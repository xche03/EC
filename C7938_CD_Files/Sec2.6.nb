   RowBox[{" ", 
    FormBox[
     RowBox[{
      SubscriptBox["\[Rho]", 
       StyleBox["xy",
        FontSlant->"Italic"]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"Cov", 
