Cell[TextData[{
 StyleBox["20. Theorem 4 can be extended slightly to include the case where ",
  FontColor->GrayLevel[0]],
 StyleBox["g",
  FontSlant->"Italic",
  FontColor->GrayLevel[0]],
 StyleBox[" is a function of both ",
  FontColor->GrayLevel[0]],
 StyleBox["X",
  FontSlant->"Italic",
  FontColor->GrayLevel[0]],
 StyleBox[" and ",
  FontColor->GrayLevel[0]],
 StyleBox["Y",
  FontSlant->"Italic",
  FontColor->GrayLevel[0]],
 StyleBox["; simply replace the random variable ",
  FontColor->GrayLevel[0]],
 StyleBox["X",
  FontSlant->"Italic",
  FontColor->GrayLevel[0]],
 StyleBox[" by its observed value ",
  FontColor->GrayLevel[0]],
 StyleBox["x",
  FontSlant->"Italic",
  FontColor->GrayLevel[0]],
 StyleBox[" as follows: ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["E",
      FontSlant->"Plain"], "[", 
     RowBox[{
      RowBox[{
       RowBox[{"g", "(", 
        RowBox[{"X", ",", "Y"}], ")"}], "|", "X"}], "=", "x"}], "]"}], "=", 
    RowBox[{
     RowBox[{
      StyleBox["E",
       FontSlant->"Plain"], "[", 
      RowBox[{
       RowBox[{
        RowBox[{"g", "(", 
         RowBox[{"x", ",", "Y"}], ")"}], "|", "X"}], "=", "x"}], "]"}], "=", 
     RowBox[{
      FormBox[
       UnderscriptBox["\[Sum]", "y"],
       TraditionalForm], 
      RowBox[{
       RowBox[{"g", "(", 
        RowBox[{"x", ",", "y"}], ")"}], 
       RowBox[{"q", "(", 
        RowBox[{"y", "|", "x"}], ")"}]}]}]}]}], TraditionalForm]],
  FontColor->GrayLevel[0]],
 ".",
 StyleBox[" Use this to show that ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["E",
      FontSlant->"Plain"], "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"h", "(", "X", ")"}], 
        RowBox[{"g", "(", "Y", ")"}]}], "|", "X"}], "=", "x"}], "]"}], "=", 
    RowBox[{
     RowBox[{"h", "(", "x", ")"}], 
     RowBox[{
      StyleBox["E",
       FontSlant->"Plain"], "[", 
      RowBox[{
       RowBox[{
        RowBox[{"g", 
         RowBox[{"(", "Y", ")"}]}], "|", "X"}], "=", "x"}], "]"}]}]}], 
   TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[", that is, given the value of ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox["X", TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[", functions of ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
