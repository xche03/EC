Cell[TextData[{
 "\tThis histogram is the observed distribution of the sample mean ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["X", "_"], TraditionalForm]]],
 " in our simulation. Recall that the mean of the underlying geometric \
distribution is ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"1", "/", "2"}], 
    RowBox[{"1", "-", 
     RowBox[{"1", "/", "2"}]}]], TraditionalForm]]],
 " = 1, and we do see that the center of the frequency histogram is roughly \
at ",
 StyleBox["x",
  FontSlant->"Italic"],
 " = 1. \n\tTo see the effect of increasing the sample size of each random \
sample, let's try quadrupling the sample size from 20 to 80."
}], "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"SeedRandom", "[", "7531", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"sample", "=", 
   RowBox[{"SimulateSampleMeans", "[", 
    RowBox[{"100", ",", 
     RowBox[{"GeometricDistribution", "[", ".5", "]"}], ",", "80"}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{"Histogram", "[", 
  RowBox[{"sample", ",", "10"}], "]"}]}], "Input",
 CellFrame->True,
 InitializationCell->True,
