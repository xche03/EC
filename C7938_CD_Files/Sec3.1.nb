Cell[TextData[{
 StyleBox["Example 6",
  FontWeight->"Bold"],
 "  Consider the sample space ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalOmega]", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", ",", "y"}], ")"}], "\[Element]", 
