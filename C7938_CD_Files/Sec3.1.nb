Cell[TextData[{
 StyleBox["11. A random variable ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox["X", TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[" mapping a sample space \[CapitalOmega] with a \[Sigma]-algebra \
\[ScriptCapitalH] to \[DoubleStruckCapitalR] is called ",
  FontColor->GrayLevel[0]],
 StyleBox["measurable with respect to",
  FontSlant->"Italic",
  FontColor->GrayLevel[0]],
 StyleBox[" \[ScriptCapitalH] if for every Borel subset ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox["B", TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[" of \[DoubleStruckCapitalR], the inverse image ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["X", 
      RowBox[{"-", "1"}]], "(", "B", ")"}], "=", 
    RowBox[{"{", 
     RowBox[{"\[Omega]", ":", 
      RowBox[{
       RowBox[{"X", "(", "\[Omega]", ")"}], "\[Element]", "B"}]}], "}"}]}], 
   TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[" is in \[ScriptCapitalH].  Consider the collection of all sets ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[ScriptCapitalH]", "x"], "=", " "}], TraditionalForm]],
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SuperscriptBox["X", 
      RowBox[{"-", "1"}]], "(", "B", ")"}], "}"}], TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[" as ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox["B", TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[" ranges through all Borel sets in \[DoubleStruckCapitalR]. Show \
that ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalH]", "x"], TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[" is a \[Sigma]-algebra with respect to which ",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox["X", TraditionalForm]],
  FontColor->GrayLevel[0]],
 StyleBox[" is measurable, and in fact it is the smallest such  \
\[Sigma]-algebra. ",
  FontColor->GrayLevel[0]]
}], "Text",
