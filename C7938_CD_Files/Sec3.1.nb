Cell[BoxData[
 RowBox[{
  RowBox[{"AssetPriceDistribution", "[", "numsteps_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "timestep", ",", "stepsize", ",", "stateprobs", ",", "statelist", ",", 
      "densitylist"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"timestep", "=", 
      RowBox[{"1", "/", "numsteps"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"stepsize", "=", 
      RowBox[{"Sqrt", "[", "timestep", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"stateprobs", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"PDF", "[", 
         RowBox[{
          RowBox[{"BinomialDistribution", "[", 
           RowBox[{"numsteps", ",", ".5"}], "]"}], ",", "k"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "0", ",", "numsteps"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"densitylist", "=", 
      RowBox[{"stateprobs", "/", 
       RowBox[{"(", 
        RowBox[{"2", "*", "stepsize"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"statelist", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "k"}], "-", "numsteps"}], ")"}], "*", "stepsize"}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "0", ",", "numsteps"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ProbabilityHistogram", "[", 
      RowBox[{"statelist", ",", "densitylist"}], "]"}]}]}], "]"}]}]], "Input",
