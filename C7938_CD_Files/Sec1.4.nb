Cell[TextData[{
 "\tYou may recognize ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["C",
     FontSlant->"Italic"], 
    RowBox[{
     StyleBox["n",
      FontSlant->"Italic"], ",", 
