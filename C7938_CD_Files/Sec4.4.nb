Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"f", "(", 
     SubscriptBox["y", "3"], ")"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"5", "!"}], 
       RowBox[{
        RowBox[{"2", "!"}], "\[CenterDot]", 
        RowBox[{"2", "!"}]}]], 
      RowBox[{
       SuperscriptBox[
        RowBox[{
         SuperscriptBox[
          SubscriptBox["y", "3"], "2"], "(", 
         RowBox[{"1", "-", 
          SubscriptBox["y", "3"]}], ")"}], "2"], "\[CenterDot]", "1"}]}], "=", 
     RowBox[{"30", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["y", "3"], "2"], "-", 
        RowBox[{"2", 
         SuperscriptBox[
          SubscriptBox["y", "3"], "3"]}], "+", 
        SuperscriptBox[
         SubscriptBox["y", "3"], "4"]}], ")"}]}]}]}], ";", " ", 
   RowBox[{
    SubscriptBox["y", "3"], "\[Element]", 
    RowBox[{"(", 
     RowBox[{"0", ",", "1"}], ")"}]}]}], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.437430723393922*^9, 3.437430723851922*^9}}],

Cell[TextData[{
 "Notice that 1/2 is the median of the distribution of each sample value, so \
it is interesting to know if the distribution of the sample median ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["X", 
    RowBox[{"(", "3", ")"}]], TraditionalForm]]],
 " puts half of its weight to the left of the distributional median ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "=", 
    RowBox[{"1", "/", "2"}]}], TraditionalForm]]],
 " and half to the right.  We compute:"
}], "Text",
